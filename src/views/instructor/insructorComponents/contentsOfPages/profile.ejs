    <div class="mt-5">
      <h1 class="display-5">Profile</h1>
      <table class="table border border-2" style="font-weight: bold; font-size:large">
        <thead style="font-weight: bold; font-size:large">
          <tr>
            <th scope="col">Basic</th>
            <th scope="col">Info</th>
          </tr>
        </thead>
        <tbody style="font-size:large">
          <tr>
            <td>Name</td>
            <td><%= dataOfInstructor.name %></td>
